<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="ListenerTestNg">
	<listeners>
		<listener
			class-name="learning.selenium.listenerTest.Listener" />
	</listeners>

	<test name="Listener" verbose="2">
		<classes>
			<class name="learning.selenium.listenerTest.LoginTest">
				<methods>
					<include name="loginByName" />
					<include name="loginByEmail" />
				</methods>
			</class>
		</classes>
	</test>
</suite> 